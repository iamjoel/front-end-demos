{
  "name": "frontend-demos",
  "version": "0.0.1",
  "description": "frontend demos",
  "main": "index.html",
  "scripts": {
    "build:js": "node_modules/.bin/webpack -p --progress --colors",
    "build": "node_modules/.bin/cross-env NODE_ENV=production npm run build:js",
    "dev": "node_modules/.bin/webpack-dev-server --inline --hot  --progress --colors --host 0.0.0.0 --port 5001",
    "dev:1": "node_modules/.bin/webpack-dev-server --config webpack.config.one.js  --inline --hot  --progress --colors --host 0.0.0.0 --port 5001",
    "ph-to-gh-pages": "node tools/publish",
    "publish": "npm run build && npm run ph-to-gh-pages",
    "preview": "node_modules/.bin/npm-run-all build start:static",
    "start:static": "node_modules/.bin/anywhere -d dist 5002",
    "create": "node tools/create-demo"
  },
  "author": "Joel",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamjoel/front-end-demos"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "2.2.3",
    "vue": "1.0.24",
    "vue-router": "0.7.13"
  },
  "devDependencies": {
    "anywhere": "1.2.0",
    "autoprefixer": "6.4.0",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "ejs-loader": "0.3.0",
    "eslint": "3.3.1",
    "eslint-config-standard": "5.3.5",
    "eslint-loader": "1.5.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "mockjs": "1.0.1-beta2",
    "npm-run-all": "1.6.0",
    "optimist": "0.6.1",
    "postcss-loader": "0.11.0",
    "precss": "1.4.0",
    "prismjs": "1.21.0",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
